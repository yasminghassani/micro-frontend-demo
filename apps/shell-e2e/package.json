{
  "name": "shell-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "shell"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/playwright:playwright",
        "outputs": [
          "{workspaceRoot}/dist/.playwright/apps/shell-e2e"
        ],
        "options": {
          "config": "apps/shell-e2e/playwright.config.ts"
        }
      },
      "lint": {
        "executor": "@nx/eslint:lint"
      }
    }
  }
}
