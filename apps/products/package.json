{
  "name": "products",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "products",
    "sourceRoot": "apps/products/src",
    "targets": {
      "build": {
        "executor": "@nx/webpack:webpack",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "compiler": "babel",
          "outputPath": "apps/products/dist",
          "index": "apps/products/src/index.html",
          "baseHref": "/",
          "main": "apps/products/src/main.ts",
          "tsConfig": "apps/products/tsconfig.app.json",
          "assets": [
            "apps/products/src/favicon.ico",
            "apps/products/src/assets"
          ],
          "styles": [
            "apps/products/src/styles.css"
          ],
          "scripts": [],
          "webpackConfig": "apps/products/webpack.config.ts",
          "postcssConfig": "apps/products/postcss.config.js"
        },
        "configurations": {
          "development": {
            "extractLicenses": false,
            "optimization": false,
            "sourceMap": true,
            "vendorChunk": true
          },
          "production": {
            "fileReplacements": [
              {
                "replace": "apps/products/src/environments/environment.ts",
                "with": "apps/products/src/environments/environment.prod.ts"
              }
            ],
            "optimization": true,
            "outputHashing": "all",
            "sourceMap": false,
            "namedChunks": false,
            "extractLicenses": true,
            "vendorChunk": false
          }
        },
        "dependsOn": [
          "^build",
          "typecheck"
        ]
      },
      "serve": {
        "executor": "@nx/react:module-federation-dev-server",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "products:build",
          "hmr": true,
          "port": 4202
        },
        "configurations": {
          "development": {
            "buildTarget": "products:build:development"
          },
          "production": {
            "buildTarget": "products:build:production",
            "hmr": false
          }
        },
        "dependsOn": [
          "typecheck"
        ]
      },
      "test": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/products/jest.config.cts"
        }
      },
      "serve-static": {
        "executor": "@nx/react:module-federation-static-server",
        "defaultConfiguration": "production",
        "options": {
          "serveTarget": "products:serve"
        },
        "configurations": {
          "development": {
            "serveTarget": "products:serve:development"
          },
          "production": {
            "serveTarget": "products:serve:production"
          }
        }
      }
    }
  },
  "exports": {
    "./Module": {
      "@org/source": "./src/remote-entry.ts",
      "types": "./src/remote-entry.ts",
      "import": "./src/remote-entry.ts",
      "default": "./src/remote-entry.ts"
    }
  },
  "types": "./src/remote-entry.ts"
}
